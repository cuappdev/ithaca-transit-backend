{
  "name": "tcat",
  "version": "1.0.0",
  "description": "Backend for Cornell AppDev's TCAT application",
  "main": "",
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/webpack --env.NODE_ENV=production --mode=production -p",
    "build:dev": "./node_modules/.bin/webpack --watch --env.NODE_ENV=development --progress --colors --mode=development --debug --devtool cheap-eval-source-map --output-pathinfo",
    "build:image": "docker build -t transit-node .",
    "docker:clean": "npm run ghopper:stop && docker system prune",
    "docker:reset": "npm run ghopper:stop && docker system prune --all",
    "flow": "echo 'Flow:' ; node_modules/.bin/flow ; echo 'Flow checking complete.'",
    "flow:stop": "node_modules/.bin/flow stop",
    "ghopper": "sh ./scripts/run_ghopper.sh",
    "ghopper:kill": "docker-compose -f docker-compose/docker-compose.yml kill ; printf ''",
    "ghopper:stop": "docker-compose -f docker-compose/docker-compose.yml stop ; printf ''",
    "init": "npm install && npm run build",
    "serve": "node --require dotenv/config build/bundle.js",
    "setup": "sh ./scripts/run.sh --setup",
    "start:dev": "sh ./scripts/run.sh --development",
    "start:prod": "npm run init && npm run serve",
    "test": "npm run init && node ./node_modules/.bin/jest --verbose --forceExit --detectOpenHandles test ; echo 'Exiting test.'",
    "test:dev": "node $NODE_DEBUG_OPTION ./node_modules/.bin/jest --verbose --runInBand --detectOpenHandles test ; echo 'Exiting test.'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuappdev/tcat.js.git"
  },
  "author": "Cornell AppDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cuappdev/tcat.js/issues"
  },
  "homepage": "https://github.com/cuappdev/tcat.js#readme",
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "acorn": "^6.0.6",
    "body-parser": "^1.18.3",
    "csvtojson": "^2.0.8",
    "decompress-zip": "^0.3.2",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-mung": "^0.5.1",
    "gps-to-gpx": "^1.1.0",
    "interval-promise": "^1.3.0",
    "json-find": "^2.0.3",
    "json-query": "^2.2.2",
    "jsondiffpatch": "^0.3.11",
    "lru-cache": "^4.1.5",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "morgan-body": "^2.4.5",
    "qs": "^6.6.0",
    "request": "^2.88.0",
    "rotating-file-stream": "^1.4.0",
    "wait-on": "^3.2.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extra-watch-webpack-plugin": "^1.0.3",
    "flow-bin": "^0.81.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "nodemon-webpack-plugin": "^4.0.7",
    "regenerator-runtime": "^0.12.1",
    "reload": "^2.4.0",
    "supertest": "^3.4.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  }
}
