{
  "name": "tcat",
  "version": "1.0.0",
  "description": "Backend for Cornell AppDev's TCAT application",
  "main": "",
  "private": true,
  "scripts": {
    "ghopper": "sh ./scripts/run_ghopper.sh",
    "stop-ghopper": "docker-compose -f docker-compose/docker-compose.yml stop ; printf ''",
    "kill-ghopper": "docker-compose -f docker-compose/docker-compose.yml kill ; printf ''",
    "clean-docker": "npm run stop-ghopper && docker system prune",
    "reset-docker": "npm run stop-ghopper && docker system prune --all",
    "build": "./node_modules/.bin/webpack --env.NODE_ENV=production --mode=production -p",
    "build-image": "docker build -t transit-node .",
    "build-dev": "./node_modules/.bin/webpack --watch --env.NODE_ENV=development --progress --colors --mode=development --debug --devtool cheap-eval-source-map --output-pathinfo",
    "setup": "sh ./scripts/run.sh --setup",
    "init": "npm install && npm run build",
    "serve": "node --require dotenv/config build/bundle.js",
    "start-dev": "sh ./scripts/run.sh --development",
    "start-prod": "npm run init && npm run serve",
    "test-dev": "node $NODE_DEBUG_OPTION ./node_modules/.bin/jest --verbose --runInBand --detectOpenHandles test ; npm run flow ; echo 'Exiting.'",
    "test": "npm run init && node ./node_modules/.bin/jest --verbose --forceExit --detectOpenHandles test ; echo 'Exiting.'",
    "flow": "echo 'Flow:' ; node_modules/.bin/flow ; echo 'Flow checking complete.'",
    "flow-stop": "node_modules/.bin/flow stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuappdev/tcat.js.git"
  },
  "author": "Cornell AppDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cuappdev/tcat.js/issues"
  },
  "homepage": "https://github.com/cuappdev/tcat.js#readme",
  "dependencies": {
    "alarm": "^2.0.0",
    "body-parser": "^1.18.3",
    "csvtojson": "^2.0.8",
    "decompress-zip": "^0.3.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-mung": "^0.5.1",
    "gps-to-gpx": "^1.1.0",
    "json-find": "^2.0.3",
    "json-query": "^2.2.2",
    "jsondiffpatch": "^0.3.11",
    "lru-cache": "^4.1.3",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "morgan-body": "^2.4.5",
    "qs": "^6.5.1",
    "request": "^2.88.0",
    "wait-on": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extra-watch-webpack-plugin": "^1.0.3",
    "flow-bin": "^0.81.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "nodemon-webpack-plugin": "^4.0.3",
    "reload": "^2.3.1",
    "supertest": "^3.3.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  }
}
