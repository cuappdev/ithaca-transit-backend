{
  "name": "tcat",
  "version": "1.0.0",
  "description": "Backend for Cornell AppDev's TCAT application",
  "main": "",
  "scripts": {
    "build": "webpack --env.NODE_ENV=production -p",
    "build-dev": "webpack --watch --open --env.NODE_ENV=dev --progress --mode=development --debug --devtool cheap-eval-source-map --output-pathinfo",
    "serve": "node build/bundle.js",
    "setup": "sh scripts/setup.sh",
    "graph": "sh scripts/graph.sh",
    "graph-walking": "sh scripts/graph-walking.sh",
    "mapmatching": "cd map-matching && ./map-matching.sh action=start-server ; cd ..",
    "start": "npm run cleanup; npm run build && sh scripts/run.sh",
    "start-dev": "npm run setup && npm run build-dev",
    "cleanup": "killall -9 java",
    "test": "jest --runInBand --noStackTrace --detectOpenHandles src/test",
    "flow": "node_modules/.bin/flow",
    "flow-stop": "node_modules/.bin/flow stop",
    "travis": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuappdev/tcat.js.git"
  },
  "author": "Cornell AppDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cuappdev/tcat.js/issues"
  },
  "homepage": "https://github.com/cuappdev/tcat.js#readme",
  "dependencies": {
    "alarm": "^2.0.0",
    "appdev": "^0.4.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "flow-bin": "^0.80.0",
    "gps-to-gpx": "^1.1.0",
    "lru-cache": "^4.1.3",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "qs": "^6.5.1",
    "request": "^2.88.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extra-watch-webpack-plugin": "^1.0.3",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4",
    "nodemon-webpack-plugin": "^4.0.3",
    "reload": "^2.3.1",
    "supertest": "^3.3.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-node-externals": "^1.7.2"
  }
}
